<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.activities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_working_hours"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_working_hours_round"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.activities.RegisterActivity"></activity>
        <activity android:name="com.example.activities.ShiftsActivity"
                android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name="com.example.activities.AddShiftActivity"></activity>
        <activity android:name="com.example.activities.EditInfoActivity"></activity>
    </application>

</manifest>